{
    "name": "scss-colors-variables",
    "version": "1.3.1",
    "description": "Some utils for set your theming colors as variables",
    "main": "index.scss",
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "build": "node-sass --recursive --output css --source-map true --source-map-contents --importer node_modules/node-sass-package-importer/dist/cli.js index.scss",
        "watch": "npm run build --watch ",
        "npm-publish": "np --no-tests"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/danilozano413/scss-colors-variables.git"
    },
    "keywords": [
        "scss",
        "utils"
    ],
    "author": "Dani Lozano",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/danilozano413/scss-colors-variables/issues"
    },
    "homepage": "https://github.com/danilozano413/scss-colors-variables#readme",
    "devDependencies": {
        "gulp": "^4.0.2",
        "gulp-sass": "^5.1.0",
        "node-sass": "^9.0.0",
        "node-sass-package-importer": "^5.3.3",
        "np": "^8.0.4"
    }
}